[
    Conditional=PUSH_API,
    NoInterfaceObject,
    RuntimeEnabled=PushRegistration,
    WillBeGarbageCollected,
] interface PushRegistrationManager {
    [CallWith=ExecutionContext, ImplementedAs=registerPush] Promise register();
    [CallWith=ExecutionContext, ImplementedAs=unregisterPush] Promise unregister();
    [CallWith=ExecutionContext, ImplementedAs=isRegisteredPush] Promise isRegistered();
    [CallWith=ExecutionContext, ImplementedAs=hasPermissionPush] Promise hasPermission();
};
